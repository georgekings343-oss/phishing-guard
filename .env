# ===== FRONTEND CONFIG =====
# The backend API base URL that the Vite frontend will call
VITE_API_BASE=http://localhost:5000

# Frontend development server port (Vite)
VITE_PORT=5173

# ===== BACKEND CONFIG =====
# Express server port
PORT=5000

# MongoDB connection
MONGO_URI=mongodb://localhost:27017/phishing_guard

# JWT configuration (used for authentication)
JWT_SECRET=supersecretkey-change-this-in-production
JWT_EXPIRES_IN=7d

# Environment mode
NODE_ENV=development

# URL of the frontend app for CORS configuration
FRONTEND_URL=http://localhost:5173

# ===== OPTIONAL EXTERNAL SERVICES =====
VITE_SUPABASE_URL=https://dummy.supabase.co
VITE_SUPABASE_ANON_KEY=dummykey.updateyourkkey.here

VITE_OPENAI_API_KEY=your-openai-api-key-here
VITE_GEMINI_API_KEY=your-gemini-api-key-here
VITE_ANTHROPIC_API_KEY=your-anthropic-api-key-here

VITE_GOOGLE_ANALYTICS_ID=your-google-analytics-id-here
VITE_ADSENSE_ID=your-adsense-id-h
